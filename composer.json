{
    "name": "sprintf/admin-bundle",
    "description": "Symfony Admin Bundle by Sprint-F.RU",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "Admin",
        "Dashboard"
    ],
    "authors": [
        {
            "name": "Albert Stepantsev",
            "email": "albert.stepantsev@sprintf.ru"
        }
    ],
    "homepage": "https://sprintf.ru",
    "require": {
        "php": "^8.1",
        "sprintf/doctrine-datetime-bundle": "^0.1",
        "sprintf/workflow-bundle": "^0.1.3",
        "symfony/dependency-injection": "*",
        "symfony/form": "*",
        "symfony/routing": "*",
        "symfony/service-contracts": "*",
        "symfony/string": "*",
        "symfony/translation": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.26"
    },
    "conflict": {
    },
    "suggest": {
        "ext-pdo": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SprintF\\Bundle\\Admin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        },
        "sort-packages": true
    }
}
